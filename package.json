{
	"name": "elements",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"packages/*",
		"private/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"test": "turbo run test",
		"format": "prettier --write \"**/*.{css,html,json,ts,yaml,yml}\""
	},
	"devDependencies": {
		"eslint-config-custom": "workspace:*",
		"prettier": "latest",
		"prettier-plugin-organize-attributes": "^1.0.0",
		"turbo": "latest"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"packageManager": "pnpm@7.21.0"
}
